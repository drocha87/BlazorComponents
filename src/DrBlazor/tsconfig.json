{
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["dom", "dom.iterable", "ES2015.Promise"],
    "module": "ES2015",
    "sourceMap": true,
    "removeComments": true,
    "noEmitOnError": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,

    "rootDirs": ["./Components", "./Extensions"],
    "outDir": "./wwwroot"
  }
}
